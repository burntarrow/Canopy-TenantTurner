{
  "name": "michelmany/canopy-tenant-turner-api",
  "description": "Canopy Tenant Turner API",
  "type": "worpress-plugin",
  "license": "GPL-2.0+",
  "require": {
    "htmlburger/carbon-fields": "^3.6",
    "guzzlehttp/guzzle": "^7.0",
    "timber/timber": "^2.0"
  },
  "require-dev": {
    "10up/wp_mock": "^1.1",
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "Inc\\": "./inc"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "platform-check": false,
    "platform": {
      "php": "7.4.0"
    },
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
